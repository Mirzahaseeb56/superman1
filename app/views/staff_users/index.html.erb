<h1>List of staff users</h1>
<table class="table">
<thead>
  <tr>
    <th scope="col">Username</th>
    <th scope="col">First</th>
    <th scope="col">Last</th>
    <th scope="col">Email</th>
    <th scope="col">Role</th>
    <th scope="col">Action</th>
  </tr>
</thead>
<tbody>
<% @staff_users.each do |staff_user| %>
  <tr>
   
    <td><%= staff_user.username %> </td>
    <td><%= staff_user.firstname %></td>
    <td><%= staff_user.lastname %></td>
    <td><%= staff_user.email %></td>
    <td><%= staff_user.role&.name %><</td>
  </tr>
<% end %>
</tbody>
</table>